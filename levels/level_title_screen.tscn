[sub_resource type="GDScript" id="GDScript_nkytd"]
script/source = "extends Node2D

func _input(event: InputEvent) -> void:
	if event is InputEventMouseMotion: return
	if event is InputEventScreenTouch: return

	if event is InputEventAction: _start_game()
	if event is InputEventFromWindow: _start_game()
	if event is InputEventJoypadButton: _start_game()
	if event is InputEventJoypadMotion: _start_game()
	if event is InputEventMouseButton: _start_game()

func _start_game():
	GlobalMusicPlayer.start_music()
	LevelRotator.story_level(self)
"

